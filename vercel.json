{
  "buildCommand": "bash scripts/build-vercel.sh",
  "outputDirectory": "dist/public",
  "functions": {
    "api/index.cjs": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.cjs"
    },
    {
      "source": "/frame",
      "destination": "/api/index.cjs"
    }
  ],
  "headers": [
    {
      "source": "/frame",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        }
      ]
    }
  ]
}
